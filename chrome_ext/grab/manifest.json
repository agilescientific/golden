{
  "manifest_version": 2,

  "name": "locati.fi",
  "description": "Do awesome things with smart spatial metadata",
  "version": "1.0",
  "icons": {"256": "icon_256.png",
            "128": "icon_128.png"
  },
  "options_ui": {
    // Required.
    "page": "options.html",
    // Recommended.
    "chrome_style": true
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/",
    "<all_urls>"
  ]
}
